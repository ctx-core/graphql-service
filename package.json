{
	"name": "@ctx-core/graphql-service",
	"version": "6.0.73",
	"description": "ctx-core graphql-service",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/graphql-service#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/graphql-service/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/graphql-service/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"module": "./dist/index.js",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.32",
		"@ctx-core/graphql": "workspace:^9.0.40",
		"@ctx-core/object": "workspace:^21.0.30",
		"apollo-server-express": "preview",
		"graphql": "^16.1.0",
		"graphql-upload": "^13.0.0"
	},
	"devDependencies": {
		"@ctx-core/function": "workspace:^20.2.12",
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"@types/graphql-upload": "^8.0.7",
		"@types/node": "^16.11.12",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"noUpdate": [
		"apollo-server-express"
	]
}
