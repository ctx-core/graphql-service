{
	"name": "@ctx-core/graphql-service",
	"version": "7.1.177",
	"description": "ctx-core graphql-service",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/graphql-service#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/graphql-service/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/graphql-service.git"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Brian Takita",
		"url": "https://briantakita.me",
		"email": "info+ctx-core-graphql-service@briantakita.me"
	},
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./_types",
		"./express_graphql_upload",
		"./polka_graphql_upload"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm run /^test:/",
		"test:types": "check-dts",
		"test:unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"disable:test:unit:coverage": "c8 pnpm test:unit"
	},
	"dependencies": {
		"@apollo/server": "^4.10.2",
		"@ctx-core/graphql": "^9.1.170",
		"ctx-core": "^7.1.1",
		"express": "^5.0.0-beta.2",
		"graphql": "^16.8.1",
		"graphql-upload": "^16.0.2"
	},
	"devDependencies": {
		"@types/express": "^4.17.21",
		"c8": "^9.1.0",
		"check-dts": "^0.7.2",
		"tsx": "^4.7.1",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"noUpdate": [
		"apollo-server-express"
	],
	"sideEffects": false
}