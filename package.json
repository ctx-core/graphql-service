{
	"name": "@ctx-core/graphql-service",
	"version": "6.0.8",
	"description": "ctx-core graphql-service",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/graphql-service#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/graphql-service/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/graphql-service/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"require": "./dist/index.cjs",
		"import": "./dist/index.js"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"require = require('esm')(module)\nmodule.exports = require('./index.js')\" > index.cjs",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.8",
		"@ctx-core/graphql": "workspace:^9.0.8",
		"@ctx-core/object": "workspace:^21.0.7",
		"apollo-server-express": "preview",
		"esm": "^3.2.25",
		"graphql": "^15.5.1",
		"graphql-upload": "^12.0.0"
	},
	"devDependencies": {
		"@ctx-core/function": "workspace:^20.0.7",
		"@types/graphql-upload": "^8.0.6",
		"@types/node": "^16.3.2",
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"noUpdate": [
		"apollo-server-express"
	]
}
