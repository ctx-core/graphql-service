		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@apollo/server": "^4.7.4",
		"@ctx-core/array": "^26.12.43",
		"@ctx-core/graphql": "^9.0.169",
		"@ctx-core/object": "^28.0.2",
		"express": "^4.18.2",
		"graphql": "^16.6.0",
		"graphql-upload": "^16.0.2"
	},
	"devDependencies": {
		"@ctx-core/function": "^21.18.0",
		"@types/express": "^4.17.17",
		"c8": "^8.0.0",
		"check-dts": "^0.7.2",
		"tsx": "^3.12.7",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"noUpdate": [
		"apollo-server-express"
	],
	"svelte": "./src/index.js",
	"sideEffects": false
