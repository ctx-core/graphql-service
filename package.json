{
	"name": "@ctx-core/graphql-service",
	"version": "5.0.4",
	"description": "ctx-core graphql-service",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/graphql-service#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/graphql-service/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/graphql-service/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./src/index.js",
	"module": "./src/index.js",
	"types": "./src/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.js && rm -f dist/**/*.d.ts && rm -f dist/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"exec": "$@",
		"prepublishOnly": "npm run compile"
	},
	"dependencies": {
		"@ctx-core/graphql": "workspace:^8.0.4",
		"@ctx-core/object": "workspace:^20.0.3",
		"apollo-server-express": "preview",
		"graphql": "^15.5.0",
		"graphql-upload": "^12.0.0"
	},
	"devDependencies": {
		"@ctx-core/function": "workspace:^19.1.2",
		"@types/graphql-upload": "^8.0.5",
		"@types/node": "^15.12.4",
		"typescript": "^4.3.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"noUpdate": [
		"apollo-server-express"
	]
}