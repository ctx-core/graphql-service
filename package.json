{
	"name": "@ctx-core/graphql-service",
	"version": "7.1.19",
	"description": "ctx-core graphql-service",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/graphql-service#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/graphql-service/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/graphql-service.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"files": [
		"*.d.ts",
		"*.js",
		"*.json",
		"./_types",
		"./express_graphql_upload",
		"./polka_graphql_upload"
	],
	"types": "./index.d.ts",
	"exports": {
		".": "./index.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": ":",
		"clean": ":",
		"exec": "$@",
		"prepublishOnly": "pnpm clean && pnpm build && pnpm test",
		"test": "pnpm test-unit && check-dts",
		"test-unit": "tsx node_modules/uvu/bin.js . '\\.test\\.(ts|js)$'",
		"test-unit-coverage": "c8 pnpm test-unit"
	},
	"dependencies": {
		"@apollo/server": "^4.9.5",
		"@ctx-core/array": "^27.0.19",
		"@ctx-core/graphql": "^9.1.19",
		"@ctx-core/object": "^31.1.0",
		"express": "^4.18.2",
		"graphql": "^16.8.1",
		"graphql-upload": "^16.0.2"
	},
	"devDependencies": {
		"@types/express": "^4.17.21",
		"c8": "^8.0.1",
		"check-dts": "^0.7.2",
		"tsx": "^4.1.4",
		"typescript": "next",
		"uvu": "^0.5.6"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"noUpdate": [
		"apollo-server-express"
	],
	"sideEffects": false
}