{
	"name": "@ctx-core/graphql-service",
	"version": "6.0.83",
	"description": "ctx-core graphql-service",
	"keywords": [
		"ctx-core",
		"google"
	],
	"homepage": "https://github.com/ctx-core/graphql-service#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/graphql-service/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/graphql-service.git"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rimraf dist && npm run clean_tsbuildinfo",
		"clean_tsbuildinfo": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.d.ts",
		"compile": "npm run compile_source && npm run compile_declaration",
		"compile_source": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"compile_declaration": "npm run clean_tsbuildinfo && tsc --declaration --emitDeclarationOnly --declarationDir dist",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.41",
		"@ctx-core/graphql": "workspace:^9.0.49",
		"@ctx-core/object": "workspace:^22.0.8",
		"apollo-server-express": "preview",
		"graphql": "^16.2.0",
		"graphql-upload": "^13.0.0"
	},
	"devDependencies": {
		"@ctx-core/function": "workspace:^20.2.20",
		"@swc/cli": "^0.1.55",
		"@swc/core": "^1.2.120",
		"@types/graphql-upload": "^8.0.7",
		"@types/node": "^17.0.0",
		"rimraf": "^3.0.2",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	},
	"noUpdate": [
		"apollo-server-express"
	],
	"svelte": "./dist/index.js"
}
